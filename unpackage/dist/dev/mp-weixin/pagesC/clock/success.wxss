.clock-success-page {
  padding-top: 64rpx;
  padding-bottom: 172rpx;
}
.clock-success-page .analysis-container {
  width: 622rpx;
  margin: auto;
}
.clock-success-page .analysis-content {
  border-radius: 32rpx;
  box-shadow: 0 0 0 2rpx #cfcfcf;
  overflow: hidden;
}
.clock-success-page .table {
  position: relative;
  padding-bottom: 34rpx;
}
.clock-success-page .table::after {
  content: "";
  position: absolute;
  left: 32rpx;
  right: 32rpx;
  bottom: 0;
  height: 2rpx;
  background-color: #e0e6f1;
}
.clock-success-page .table-header {
  padding: 24rpx 32rpx;
  height: 92rpx;
  background-color: #E3FFEE;
}
.clock-success-page .table-row {
  height: 56rpx;
  padding: 8rpx 32rpx;
}
.clock-success-page .table-footer {
  padding: 34rpx 32rpx 40rpx 32rpx;
}
.clock-success-page .tabbar-container {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  padding: 16rpx 32rpx env(safe-area-inset-bottom) 32rpx;
  background-color: #fff;
}
.clock-success-page .back-btn {
  width: 100%;
  height: 88rpx;
  border-radius: 44rpx;
  background-color: #30a375;
}
